<p id="notice"><%= notice %></p>

<h1>Episodes</h1>

<table>
  <thead>
    <tr>
      <th>Folio</th>
      <th>Tipo ingreso</th>
      <th>Fecha ingreso</th>
      <th>Contactos laborales</th>
      <th>Contactos no laborales</th>
      <th>Presentacion</th>
      <th>Inicio sintomas</th>
      <th>Inicio cuarentena</th>
      <th>Fin cuarentena</th>
      <th>Fin cuarentena codelco</th>
      <th>Origen </th>
      <th>Cierre</th>
      <th>Trabajador</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @episodes.each do |episode| %>
      <tr>
        <td><%= episode.folio %></td>
        <td><%= episode.tipo_ingreso %></td>
        <td><%= episode.fecha_ingreso %></td>
        <td><%= episode.contactos_laborales %></td>
        <td><%= episode.contactos_no_laborales %></td>
        <td><%= episode.presentacion %></td>
        <td><%= episode.inicio_sintomas %></td>
        <td><%= episode.inicio_cuarentena %></td>
        <td><%= episode.fin_cuarentena %></td>
        <td><%= episode.fin_cuarentena_codelco %></td>
        <td><%= episode.origen_contagio %></td>
        <td><%= episode.cierre %></td>
        <td><%= episode.trabajador_id %></td>
        <!-- <td>link_to 'Show', trabajador_episode_path(trabajador, episode) </td> -->
        <!-- <td> link_to 'Edit', edit_trabajador_episode_path(@trabajador, @episodes) </td> -->
        <!-- <td> link_to 'Destroy', episode, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Episode', new_trabajador_episode_path %>
