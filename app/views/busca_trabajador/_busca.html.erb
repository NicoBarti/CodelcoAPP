<%= form_with(model: @buscaTrabajador, method: 'post', url: buscaTrabajador_index_path, local: true) do |form| %>
<div class="form-inline">
<div class="form-group">
  <%= form.label :rut %>
  <%= form.text_field :rut , class: 'form-control' %>
</div>

  <div class="form-group">
  <%= form.label :nombres %>
  <%= form.text_field :nombres , class: 'form-control'%>
</div>

<div class="form-group">
  <%= form.label :apellido1 %>
  <%= form.text_field :apellido1 , class: 'form-control'%>
</div>

<div class="form-group">
  <%= form.label :apellido2 %>
  <%= form.text_field :apellido2 , class: 'form-control'%>
</div>

<div class="actions">
  <%= form.submit 'Buscar', class: 'btn btn-primary' %>
</div>
</div>

<% end %>
