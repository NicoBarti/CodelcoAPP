<p id="notice"><%= notice %></p>

<h1>Seguimientos</h1>

<table>
  <thead>
    <tr>
      <th>Tipo aislamiento</th>
      <th>Estado seguimiento</th>
      <th>Hospitalizacion</th>
      <th>Ventilacion</th>
      <th>Falleceresp</th>
      <th>Estadoegreso</th>
      <th>Evolucion</th>
      <th>References</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seguimientos.each do |seguimiento| %>
      <tr>
        <td><%= seguimiento.tipo_aislamiento %></td>
        <td><%= seguimiento.estado_seguimiento %></td>
        <td><%= seguimiento.hospitalizacion %></td>
        <td><%= seguimiento.ventilacion %></td>
        <td><%= seguimiento.falleceresp %></td>
        <td><%= seguimiento.estadoegreso %></td>
        <td><%= seguimiento.evolucion %></td>
        <td><%= seguimiento.references %></td>
        <td><%= link_to 'Show', seguimiento %></td>
        <td><%= link_to 'Edit', edit_seguimiento_path(seguimiento) %></td>
        <td><%= link_to 'Destroy', seguimiento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seguimiento', new_seguimiento_path %>
