<p id="notice"><%= notice %></p>

<h1>Sintomas</h1>

<table>
  <thead>
    <tr>
      <th>Inicio sintomas</th>
      <th>Fiebre</th>
      <th>Cefalea</th>
      <th>Disnea</th>
      <th>Tos</th>
      <th>Odinofagia</th>
      <th>Taquipnea</th>
      <th>Cianosis</th>
      <th>Mialgia</th>
      <th>Dolorabdominal</th>
      <th>Diarrea</th>
      <th>Anosmia</th>
      <th>Agueusia</th>
      <th>Diabetes</th>
      <th>Hipertension</th>
      <th>Otras</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sintomas.each do |sintoma| %>
      <tr>
        <td><%= sintoma.inicio_sintomas %></td>
        <td><%= sintoma.fiebre %></td>
        <td><%= sintoma.cefalea %></td>
        <td><%= sintoma.disnea %></td>
        <td><%= sintoma.tos %></td>
        <td><%= sintoma.odinofagia %></td>
        <td><%= sintoma.taquipnea %></td>
        <td><%= sintoma.cianosis %></td>
        <td><%= sintoma.mialgia %></td>
        <td><%= sintoma.dolorabdominal %></td>
        <td><%= sintoma.diarrea %></td>
        <td><%= sintoma.anosmia %></td>
        <td><%= sintoma.agueusia %></td>
        <td><%= sintoma.diabetes %></td>
        <td><%= sintoma.hipertension %></td>
        <td><%= sintoma.otras %></td>
        <td><%= link_to 'Show', sintoma %></td>
        <td><%= link_to 'Edit', edit_sintoma_path(sintoma) %></td>
        <td><%= link_to 'Destroy', sintoma, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

